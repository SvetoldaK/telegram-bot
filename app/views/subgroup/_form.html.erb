<%= form_with model: @subgroup, url: user_subgroup_index_path(@user.id) do |form| %>
  <div>
    <%= form.label t('.name') %><br>
    <%= form.text_field :name %><br>
  </div>

  <div>
    <%= form.label t('.nickname') %><br>
    <%= form.text_area :nickname %><br>
  </div>

  <div>
    <%= form.submit t('.subgroup')%>
  </div>
  
<% end %>